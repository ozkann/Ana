<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model" auto-import="true">
<class name="Products" table="products">
		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>
		<id name="stock_id" type="int" column="stock_id">
			<generator class="native"/>
		</id>
	<property name="category_id" column="category_id" type="int"/>
	<property name="product_name" column="product_name" type="string"/>
	<property name="size" column="size" type="string"/>
	<property name="purchase" column="purchase" type="float"/>
	<property name="sale" column="sale" type="float"/>
	<property name="sDate" column="SDate" type="date"/>
	
	
	
</class>
</hibernate-mapping>